<p><%=apiary[0].apiary_name %></p>

<table border="3px">
    <tr>
        <td>Название</td>
        <td>Гнёзд</td>
        <td>Магазинов</td>
        <td>Характер</td>
        <td>Возраст матки</td>
    </tr>

    <% apiary.forEach((hive) => { %>
        <tr>
            <td> <a href=""> <%=hive.hive_name %> </a> </td>
            <td><%=hive.nest_count %></td>
            <td><%=hive.store_count %></td>
            <td><%=hive.bee_nature %></td>
            <td><%=hive.queen_age %> год(а)</td>
        </tr>
    <% }) %>
    
</table>