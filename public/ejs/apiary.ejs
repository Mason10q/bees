<p><%=apiary[0].apiary_name %></p>

<img src="<%=apiary[0].apiary_photo_url%>"/>

<table border="3px">
    <tr>
        <td>Название</td>
        <td>Гнёзд</td>
        <td>Магазинов</td>
        <td>Характер</td>
        <td>Возраст матки</td>
    </tr>

    <% apiary.forEach((hive) => { %>
        <tr>
            <td onclick="window.location.assign(`/profile/apiary/hive?hive_id=<%= hive.hive_id %>`)"> <%=hive.hive_name %> </td>
            <td><%=hive.nest_amount %></td>
            <td><%=hive.store_amount %></td>
            <td><%=hive.bee_nature %></td>
            <td><%=hive.queen_age %> год(а)</td>
        </tr>
    <% }) %>
    
</table>